<h1>Listings</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Location</th>
    <th>Price</th>
    <th>Details</th>
  </tr>

  <% @places.each do |place| %>
    <tr>
      <td><%= place.title %></td>
      <td><%= place.description %></td>
      <td><%= place.location %></td>
      <td><%= place.price %></td>
      <td><%= link_to 'Show', user_place_path(@user, place) %></td>
    </tr>
  <% end %>
</table>
      
